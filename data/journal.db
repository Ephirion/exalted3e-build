:""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":-50000,"name":"Crystal","type":"text","_id":"eND9RlSUeX8t0Amv","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<section class=\"journal-page-content\" style=\"box-sizing:border-box;user-select:text;color:rgb(25, 24, 19);font-family:Roboto, sans-serif;font-size:14px;font-style:normal;font-variant-ligatures:normal;font-variant-caps:normal;font-weight:400;letter-spacing:normal;orphans:2;text-align:start;text-indent:0px;text-transform:none;white-space:normal;widows:2;word-spacing:0px;-webkit-text-stroke-width:0px;text-decoration-thickness:initial;text-decoration-style:initial;text-decoration-color:initial\"><p style=\"box-sizing:border-box;user-select:text;margin:0.5em 0px\"><span style=\"font-family:Roboto, sans-serif\">No writeup, on main map</span></p></section>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":-100000,"name":"Ascension","type":"text","_id":"m9gf2WLlS4UGPvxm","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p><span style=\"font-family:Roboto, sans-serif\">Main Writeup: (Exalted, p. 85)</span></p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":1250000,"name":"Tusk","type":"text","_id":"qjtp7Sz4jJQeeaPb","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>Main Writeup: (Exalted, p. 85)</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}}],"folder":"YIyy6jACgIANeJ3f","sort":125000,"ownership":{"default":0,"ApGnoXXYFOfh6eK0":3,"vfXXFo7RKBqUZ1bk":3},"flags":{"scene-packer":{"hash":"be50497f3f80bf0eb5ca5155ae695ae338821944"}},"_stats":{"systemId":"exaltedthird","systemVersion":"1.12.2","coreVersion":"10.291","createdTime":1678930411764,"modifiedTime":1686